@model IEnumerable<New>

@{
    ViewData["Title"] = "Home Page";
}

@foreach(var item in Model)
{
    <div class="col-md-12">
        <h2>@item.Title</h2>
        <hr>
        <img class="img-thumbnail" src="~/NewsImages/@item.ImageName"/>
        <p>
            @item.Description
        </p>
        <p>
            @item.CreateDate
        </p>
    </div>
}